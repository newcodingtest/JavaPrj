package OOP;

public class Pocketmon {
	
	//1.field
	//[타입] String type ,[이름] String name ,[체력] int hp,
	//[공격력] int attack, [번호] int number
	//[필살기] String last_attack
	
	private String type;
	private String name;
	private int hp;
	private int attack;
	private int number;
	private String last__attack;
	
	//2.creator
	
    public Pocketmon(String type,String name,int hp,int attack,int number,String last_attack){
    	this.type=type;
    	this.name=type;
    	this.hp=hp;
    	this.attack=attack;
    	this.number=number;
    	this.last_attack=last_attack;
    	
    	
    	
    }
	
	//3.method

}
