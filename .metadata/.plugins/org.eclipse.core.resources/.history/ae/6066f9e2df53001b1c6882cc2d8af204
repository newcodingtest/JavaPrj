package Abstract;

public class RegularEmployee {
	private String empno;
	private String name;
	private int pay;
	private int bonus;
	
	public RegularEmployee(String empno, String name, int pay, int bonus) {
		
		this.empno = empno;
		this.name = name;
		this.pay = pay;
		this.bonus = bonus;
	}

	public int getMoneyPay() {
		
		return (pay+bonus)/12;
	}
	
	public String print(){
		return this.empno+":"+this.name+":"+this.pay;
		
	}

	
	
	

}
