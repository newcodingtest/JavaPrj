package OOP;

public class STUDENT {
  
  String name;
   String number;
   int age;
   int scorejava;
   int scoreWeb;
   int scoreAndroid;
   
   public void show() {
	   System.out.println("HI");
	   System.out.println(number+" , "+age);
	   System.out.println("윤주영님의 java점수는"+scorejava +"입니다");
	   System.out.println("윤주영님의 web점수는"+scoreWeb +"입니다");
	   System.out.println("윤주영님의 android점수는"+scoreAndroid +"입니다");
	   System.out.println("윤주영님의 java점수는"+scorejava +"입니다");
   }
}
